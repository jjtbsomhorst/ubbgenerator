{
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "require": {
        "slim/slim": "4.*",
        "slim/psr7": "^1.5",
        "php-di/php-di": "^6.3",
        "selective/basepath": "^2.1",
        "zircote/swagger-php": "^4.2",
        "kerox/tmdb": "^0.4.0",
        "monolog/monolog": "^2.3",
        "symfony/serializer": "^6.0",
        "symfony/property-access": "^6.0",
        "jjtbsomhorst/omdbapi": "dev-master",
        "guzzlehttp/guzzle": "^7.4",
        "doctrine/annotations": "^1.13",
        "phpdocumentor/reflection-docblock": "^5.3",
        "symfony/cache": "^6.0",
        "kevinrob/guzzle-cache-middleware": "^3.5",
        "psr/cache": "^3.0",
        "doctrine/orm": "2.11.1",
        "doctrine/dbal": "^3.3",
        "selective/validation": "^2.0",
        "cakephp/validation": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jjtbsomhorst/omdbapi",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/jjtbsomhorst/omdbapiclient.git",
                    "type": "git",
                    "reference": "main"
                },
                "autoload": {
                    "psr-4" : {
                        "jjtbsomhorst\\omdbapi\\" : "src"
                    }
                }
            }
        }
    ]
}
